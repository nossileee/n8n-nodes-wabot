      "dist/nodes/Wabot/WabotSendMedia.node.js",
      "dist/nodes/Wabot/WabotSendGroupText.node.js",
      "dist/nodes/Wabot/WabotSendGroupMedia.node.js",
      "dist/nodes/Wabot/WabotGroupAddNumber.node.js",
      "dist/nodes/Wabot/WabotGroupRemoveNumber.node.js",
      "dist/nodes/Wabot/WabotContactGroupAdd.node.js",
      "dist/nodes/Wabot/WabotContactGroupRemove.node.js",
      "dist/nodes/Wabot/WabotLabelAdd.node.js",
      "dist/nodes/Wabot/WabotLabelRemove.node.js"
    ]
  },
  "files": [
    "dist",
    "icons",
    "README.md",
    "LICENSE",
    "package.json",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webinfinitebr/n8n-nodes-wabot.git"
  },
  "bugs": {
    "url": "https://github.com/webinfinitebr/n8n-nodes-wabot/issues"
